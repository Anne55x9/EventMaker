<Page
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:EventMaker.View"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:Viewmodel="using:EventMaker.Viewmodel"
    x:Class="EventMaker.View.EventPage"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}" Tag="{Binding}">
        <Grid.DataContext>
            <Viewmodel:EventViewModel/>
        </Grid.DataContext>
        <ListView x:Name="listView" HorizontalAlignment="Left" Height="700" Margin="95,180,0,0" VerticalAlignment="Top" Width="380" ItemsSource="{Binding Singleton.ObservableCollectionEvent}"/>
        <ListView x:Name="listView1" HorizontalAlignment="Left" Height="100" Margin="1285,945,0,-85" VerticalAlignment="Top" Width="100"/>

    </Grid>
</Page>
